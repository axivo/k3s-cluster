  "enabledManagers": [
    "ansible-galaxy",
    "custom.regex",
    "github-actions"
  ],
  "extends": [
    ":semanticCommitTypeAll(chore)",
    "config:recommended"
  ],
  "gitIgnoredAuthors": [
    "41898282+github-actions[bot]@users.noreply.github.com"
  ],
  "labels": [
    "dependency"
  ],
  "packageRules": [
    {
      "enabled": true,
      "matchDatasources": [
        "deb"
      ],
      "matchDepNames": [
        "buildkite/helm"
      ],
      "matchPackageNames": [
        "helm"
      ],
      "registryUrls": [
        "https://packages.buildkite.com/helm-linux/helm-debian/any/?binaryArch=amd64&components=main&suite=any"
      ],
      "sourceUrl": "https://buildkite.com/organizations/helm-linux/packages/registries/helm-debian"
    },
    {
      "enabled": true,
      "matchDatasources": [
        "node-version"
      ],
      "matchDepNames": [
        "node"
      ],
      "matchUpdateTypes": [
        "major"
      ]
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "semanticCommits": "enabled",
  "separateMajorMinor": false,
  "separateMinorPatch": false
}
